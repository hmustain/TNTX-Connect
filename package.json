{
  "name": "tntx-connect",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
  "start:redis": "redis-server",
  "start:server": "cd server && npm start",
  "start:client": "cd client && npm start",
